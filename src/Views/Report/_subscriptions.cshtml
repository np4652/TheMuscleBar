@model IEnumerable<TheMuscleBar.AppCode.Reops.Entities.SubscripitionReport>

@if (Model != null && Model.Any())
{
    int i = 0;
    foreach (var item in Model)
    {
        i++;
        <tr>
            <td>@i</td>
            <td>@item.UserName</td>
            <td>@item.PhoneNumber</td>
            <td>@item.DateFrom</td>
            <td>@item.DateTo</td>
        </tr>
    }
}