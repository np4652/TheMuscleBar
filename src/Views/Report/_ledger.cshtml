@model IEnumerable<TheMuscleBar.AppCode.Reops.Entities.Legder>

@if (Model != null && Model.Any())
{
    int i = 0;
    foreach (var item in Model)
    {
        i++;
        <tr>
            <td>@i</td>
            <td>@item.UserName [@item.PhoneNumber]</td>
            <td>@item.TransactionType</td>
            <td>@item.Amount</td>
            <td>@item.Discount</td>
            <td>@item.PaymentMode</td>
            <td>@item.EntryOn</td>
        </tr>
    }
}